{
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "local": "ng serve --configuration=local",
    "dev": "ng serve --configuration=development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "cover": "ng test --no-watch --code-coverage",
    "eslint": "eslint ./src --quiet",
    "lint": "sh ./bash-scripts/lint.sh",
    "testing": "sh ./bash-scripts/cover.sh"
  },
  "private": true,
